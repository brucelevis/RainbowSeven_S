<root>

    <Properties>
        <Name>
            <Type>      UNICODE         </Type>
            <Flags>     ALL_CLIENTS     </Flags>
            <Persistent>	true	    </Persistent>
        </Name>
        <RoleType>
            <Type>      UINT8         </Type>
            <Flags>     ALL_CLIENTS     </Flags>
            <Persistent>	true	    </Persistent>
        </RoleType>

        <!-- 游戏数据 -->
        <BaseHP>    <!-- 初始血量 -->
