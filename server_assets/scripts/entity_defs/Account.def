<root>
    <Properties>
        <Level>
            <Type> UINT16 </Type>
            <Flags> BASE_AND_CLIENT </Flags>
            <Default> 1 </Default>
            <Persistent> true </Persistent>
        </Level>

        <Exp>
            <Type> UINT64 </Type>
            <Flags> BASE_AND_CLIENT </Flags>
            <Default> 0 </Default>
            <Persistent> true </Persistent>
        </Exp>

        <Fame>
            <Type> UINT64 </Type>
            <Flags> BASE_AND_CLIENT </Flags>
            <Default> 0 </Default>
            <Persistent> true </Persistent>
        </Fame>

        <Coin>
            <Type> UINT64 </Type>
            <Flags> BASE_AND_CLIENT </Flags>
            <Default> 10000 </Default>
            <Persistent> true </Persistent>
        </Coin>

        <RoleList>
            <Type> ROLE_LIST </Type>
            <Flags> BASE </Flags>
            <Persistent> true </Persistent>
        </RoleList>
    </Properties>

    <ClientMethods>
        <OnReqAccountInfo>
            <Arg> UINT16 </Arg> 
            <Arg> UINT64 </Arg>
            <Arg> UINT64 </Arg>
            <Arg> UINT64 </Arg>
        </OnReqAccountInfo>

        <!-- 请求干员信息列表回调 -->
        <OnReqRoleList>
            <Arg> ROLE_LIST </Arg>
        </OnReqRoleList>

        <!--请求解锁干员回调 -->
        <OnReqUnlockRole>
            <Arg>  UINT8  </Arg> <!--结果-->
        </OnReqUnlockRole>
    </ClientMethods>

    <BaseMethods>
        <!-- 请求账户信息：等级，经验，声望值，金币 -->
        <ReqAccountInfo>
        <Exposed/>
        </ReqAccountInfo>

        <!-- 客户端请求干员信息列表 -->
        <ReqRoleList>
        <Exposed/>
        </ReqRoleList>

        <!-- 客户端请求解锁干员 -->
        <ReqUnlockole>
            <Exposed/>
            <Arg>    UINT8    </Arg> <!-- 角色类型RoleType -->
        </ReqUnlockole>
    </BaseMethods>

    <CellMethods>
    </CellMethods>
</root>
